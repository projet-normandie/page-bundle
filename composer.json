{
    "name": "projet-normandie/page-bundle",
    "description": "Normandie PageBundle",
    "type": "symfony-bundle",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Benard David",
            "email": "magicbart@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "a2lix/translation-form-bundle": "~3.0",
        "api-platform/api-pack": "*",
        "sonata-project/admin-bundle": "^4.0",
        "sonata-project/doctrine-orm-admin-bundle": "^4.0",
        "sonata-project/translation-bundle": "^3.0",
        "knplabs/doctrine-behaviors": "~2.0"
    },
    "suggest": {
        "sonata-project/admin-bundle": "In order to use Admin interface",
        "sonata-project/doctrine-orm-admin-bundle": "In order to use Admin interface"
    },
    "autoload": {
        "psr-4": { "ProjetNormandie\\PageBundle\\": "src/" }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
